<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Scaleout.Collections</id>
    <version>1.0.4</version>
    <releaseNotes>Fixes internal bookkeeping error in RecentDictionary when there's a hash collision between the item being evicted and the item being added.</releaseNotes>
    <title>ScaleOut Software Collections for .NET</title>
    <authors>scaleoutsoftware</authors>
    <owners>scaleoutsoftware</owners>
	<license type="expression">Apache-2.0</license>
    <projectUrl>https://github.com/scaleoutsoftware/ScaleoutCollectionsDotNet</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A library of .NET dictionaries that are useful building blocks for in-memory caches with various eviction strategies (LRU and Random, primarily).</description>
    <copyright>Copyright (C), 2022 by ScaleOut Software, Inc.</copyright>
    <tags>LRU Random eviction cache ordered dictionary</tags>
	<dependencies>
      <group targetFramework="net45">
      </group>
    
      <group targetFramework="netstandard2.0">
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- Cross-platform -->
    <file src="..\NetStandard\bin\Release\netstandard2.0\Scaleout.Collections.dll" target="lib\netstandard2.0\Scaleout.Collections.dll" />
    <file src="..\NetStandard\bin\Release\netstandard2.0\Scaleout.Collections.xml" target="lib\netstandard2.0\Scaleout.Collections.xml" />

    <!-- .NET Framework 4.5 and higher -->
    <file src="..\NetFramework\bin\Release\Scaleout.Collections.dll" target="lib\net45\Scaleout.Collections.dll" />
    <file src="..\NetFramework\bin\Release\Scaleout.Collections.xml" target="lib\net45\Scaleout.Collections.xml" />
  </files>
</package>